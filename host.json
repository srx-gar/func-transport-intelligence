{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 20
      }
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Warning",
      "Host.Aggregator": "Warning",
      "Host.Triggers.Blob": "Warning",
      "Host.Triggers": "Warning",
      "Host.General": "Warning",
      "Azure.Core": "Error",
      "Azure.Storage.Blobs": "Error",
      "Azure.Storage.Queues": "Error",
      "Function": "Information",
      "Function.ztdwr_sync": "Information",
      "Function.manual_sync": "Information",
      "Function.healthz": "Information"
    },
    "fileLoggingMode": "never"
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 3,
    "minimumInterval": "00:00:05",
    "maximumInterval": "00:05:00"
  },
  "functionTimeout": "03:00:00",
  "extensions": {
    "blobs": {
      "maxDegreeOfParallelism": 4,
      "poisonBlobThreshold": 5
    },
    "grpc": {
      "maxReceiveMessageLength": 2147483647,
      "maxSendMessageLength": 2147483647
    },
    "http": {
      "routePrefix": "api",
      "dynamicThrottlesEnabled": false
    }
  },
  "healthMonitor": {
    "enabled": false
  }
}
